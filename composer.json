{
	"require": {
		"codewithkyle/jitter": "1.1.0",
		"codewithkyle/lightkeeper": "^1.1",
		"craftcms/cms": "3.5.15.1",
		"craftcms/element-api": "2.6.0",
		"craftcms/redactor": "2.8.3",
		"ether/logs": "3.0.3",
		"ether/splash": "3.0.5",
		"nystudio107/craft-eagerbeaver": "1.0.4",
		"nystudio107/craft-retour": "3.1.42",
		"sebastianlenz/linkfield": "1.0.23",
		"vaersaagod/matrixmate": "1.2.7",
		"verbb/formie": "1.2.24",
		"verbb/smith": "1.1.10",
		"verbb/super-table": "2.6.3",
		"vlucas/phpdotenv": "^3.4.0",
		"yiisoft/yii2-redis": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"modules\\papertrainmodule\\": "modules/papertrainmodule/src/"
		}
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true,
		"platform": {
			"php": "7.0"
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		]
	}
}
